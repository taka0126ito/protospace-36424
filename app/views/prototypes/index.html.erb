<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
         こんにちは、
         <%= link_to "#{current_user.name}さん", user_path(current_user.id),method: :get, class: :greeting__link %>
         
         <% else%>
         <% end %>
      </div>
    <div class="card__wrapper">
<%= render partial: "prototype", collection: @prototypes %>
    </div>
  </div>
</main>
